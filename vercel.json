{
  "rewrites": [
    {
      "source": "/_api/(.*)",
      "destination": "https://portfolio-server-omega-two.vercel.app/_api"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
